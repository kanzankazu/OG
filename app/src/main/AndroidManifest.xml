<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gandsoft.openguide">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".activity.SplashscreenActivity"
            android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.IntroActivity"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.AccountActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="Account" />
        <activity
            android:name=".activity.ChangeEventActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.main.BaseHomeActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.infomenu.aMapActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.bGalleryActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.cInboxActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.dComitteContactActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.eEmergenciesActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.fPracticalInfoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.gSurroundingAreaActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.infomenu.hFeedbackActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name=".gallery.withviewpager.PagerActivity"
            android:label="alalalal"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>